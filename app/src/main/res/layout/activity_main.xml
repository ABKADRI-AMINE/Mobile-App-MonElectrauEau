<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".WaterList">

    <Button
        android:id="@+id/btnViewConsumptions"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:text="View Electricity Consumption List" />

    <Button
        android:id="@+id/btnViewConsumptionWater"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/btnViewConsumptions"
        android:layout_marginTop="16dp"
        android:layout_centerHorizontal="true"
        android:text="View Water Consumption List" />

</RelativeLayout>
